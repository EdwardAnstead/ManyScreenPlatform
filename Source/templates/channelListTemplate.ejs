
<div id="bigDiv">

<% for (var i = 0; i< channels.length; i++) { %>
	<div class="col2Width">
		<div class="channelDiv thumbnail" id ="<%=channels[i].channelId%>">
			<img src="<%=channels[i].thumbNail%>" alt="thumbnail">
			<div class="caption">
				<h3><%=channels[i].channelName%></h3>
			</div>
		</div>
	</div>
<%}%>	

</div>